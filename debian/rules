#!/usr/bin/make -f

source:
	:

%:
	dh $@

# dh_make generated override targets
# This is example for Cmake (See https://bugs.debian.org/641051 )
override_dh_auto_configure:
	cat /proc/meminfo
	uname -a
	dh_auto_configure -- \
	-DCMAKE_CXX_FLAGS="-DOPENSSL_API_COMPAT=0x10100000L" \
	-DBUILD_ONLY="kms"
